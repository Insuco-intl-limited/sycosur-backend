<?xml version="1.0"?>
<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml"
        xmlns:jr="http://openrosa.org/javarosa"
        xmlns:odk="http://www.opendatakit.org/xforms"><h:head><h:title>Test ODK</h:title><model odk:xforms-version="1.0.0"><instance><data id="test_odk"><date/><start/><gr_enqueter><enqueteur/><enqueteur_autre/><sous_cat/><sous_cat_autre/><cible_point_gps/><point_gps/><trace_gps/><sujet_photo/><photo/></gr_enqueter><meta><instanceID/></meta></data></instance><instance id="enqueteurL"><root><item><name>enqueteurA</name><label>Enquêteur A</label></item><item><name>enqueteurB</name><label>Enquêteur B</label></item><item><name>autre</name><label>Autre enquêteur</label></item></root></instance><instance id="categorie"><root><item><name>data</name><label>Correction de données erronées</label></item><item><name>gis</name><label>Correction des géomètries erronées</label></item></root></instance><instance id="sous_cat"><root><item><name>releve_trace_gps</name><label>Relevé de tracé GPS</label><loc1>gis</loc1></item><item><name>releve_point_gps</name><label>Relevé d'un point GPS</label><loc1>gis</loc1></item><item><name>identification_pap</name><label>Identification d'une PAP</label><loc1>data</loc1></item><item><name>prise_photo</name><label>Prise de photo</label><loc1>all</loc1></item><item><name>autre</name><label>Autre</label><loc1>all</loc1></item></root></instance><instance id="ouiL"><root><item><name>oui</name><label>Oui</label></item><item><name>non</name><label>Non</label></item></root></instance><bind nodeset="/data/date" jr:preload="date" type="date" jr:preloadParams="today"/><bind nodeset="/data/start" jr:preload="timestamp" type="dateTime" jr:preloadParams="start"/><bind nodeset="/data/gr_enqueter/enqueteur" type="string"/><bind nodeset="/data/gr_enqueter/enqueteur_autre" type="string" relevant=" /data/gr_enqueter/enqueteur  = 'autre'" constraint="regex(.,'^[^&quot;,;/:$]+$')"/><bind nodeset="/data/gr_enqueter/sous_cat" type="string"/><bind nodeset="/data/gr_enqueter/sous_cat_autre" type="string" relevant="selected( /data/gr_enqueter/sous_cat ,'autre')"/><bind nodeset="/data/gr_enqueter/cible_point_gps" type="string" relevant="selected( /data/gr_enqueter/sous_cat ,'releve_point_gps') or selected( /data/gr_enqueter/sous_cat ,'releve_trace_gps')"/><bind nodeset="/data/gr_enqueter/point_gps" type="geopoint" relevant="selected( /data/gr_enqueter/sous_cat ,'releve_point_gps')"/><bind nodeset="/data/gr_enqueter/trace_gps" type="geotrace" relevant="selected( /data/gr_enqueter/sous_cat ,'releve_trace_gps')"/><bind nodeset="/data/gr_enqueter/sujet_photo" type="string" relevant="selected( /data/gr_enqueter/sous_cat ,'prise_photo')"/><bind nodeset="/data/gr_enqueter/photo" type="binary" relevant="selected( /data/gr_enqueter/sous_cat ,'prise_photo')"/><bind nodeset="/data/meta/instanceID" type="string" readonly="true()" jr:preload="uid"/></model></h:head><h:body><group appearance="field-list" ref="/data/gr_enqueter"><select1 ref="/data/gr_enqueter/enqueteur"><label>Sélectionnez l'enquêteur :</label><itemset nodeset="instance('enqueteurL')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/gr_enqueter/enqueteur_autre"><label>Préciser si autre :</label><hint>Mettre un espace entre les prénoms et les noms.</hint></input><select1 ref="/data/gr_enqueter/sous_cat"><label>Préciser la correction</label><itemset nodeset="instance('sous_cat')/root/item"><value ref="name"/><label ref="label"/></itemset></select1><input ref="/data/gr_enqueter/sous_cat_autre"><label>Expliquer la correction</label></input><input ref="/data/gr_enqueter/cible_point_gps"><label>A quoi correspond ce point/cette trace GPS ?</label></input><input ref="/data/gr_enqueter/point_gps"><label>Prendre le point GPS</label></input><input ref="/data/gr_enqueter/trace_gps"><label>Entregistrer le tracé GPS (prendre un point à chaque changement de direction et environ au milieu de chaque ligne droite).</label></input><input ref="/data/gr_enqueter/sujet_photo"><label>Qu'est-ce que vous devez photographier ?</label></input><upload ref="/data/gr_enqueter/photo" mediatype="image/*"><label>Prendre la photo</label></upload></group></h:body></h:html>